{
  "name": "teboconsulting",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sass-css": "node-sass --output-style nested --indent-type tab --indent-width 1 -o src/css src/scss",
    "prod-img": "copyfiles -u 2 src/img/* dist/img",
    "prod-html-css": "useref src/index.html dist/index.html --css concat",
    "src-s": "browser-sync start --server --files 'src/*' --startPath 'src'",
    "dist-s": "browser-sync start --server --files 'dist/*' --startPath 'dist'"
  },
  "author": "brock patterson",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^4.2.1",
    "browser-sync": "^2.26.3",
    "concat": "^1.0.3",
    "copyfiles": "^2.1.0",
    "node-sass": "^4.11.0",
    "useref-file": "^0.2.1"
  }
}
